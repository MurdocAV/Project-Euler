#Note use sieves formula for a faster result to find primes

def factors(number):

  biggestPossibleFactor = number
  factorsList = [1,number]

  for n in range(2,number + 1):

    if n >= biggestPossibleFactor:
      return factorsList

    if number % n == 0:
      factorsList.append(n) 
      biggestPossibleFactor = int(number / n)

      if biggestPossibleFactor not in factorsList:
        factorsList.append(biggestPossibleFactor)

  return factorsList

def listOfPrimes(maxNum):
    
  primesList = [2]

  for n in range(3,maxNum + 1,2):
    if len(factors(n)) == 2:
      primesList.append(n)

  return primesList

def primeFactor(number, primes):

  index = 0
  num = number
  factorDict = {}

  while num > 1:
    if num % primes[index] == 0:
        
      if primes[index] in factorDict:
        factorDict[primes[index]] += 1
      else:
        factorDict[primes[index]] = 1
      num = int(num / primes[index])
      
    else:
      index += 1
      
  formula = 1
  
  for k,v in factorDict.items():
    formula *= (v + 1)
  return formula


primes = listOfPrimes(10000)
print(primeFactor(2000000,primes))
