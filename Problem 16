def SumOfDigits(strNum):#Returns sum of all digits given a str of Number
    total = 0
    for i in range(0,len(strNum)):
        #print(strNum[i])
        total += int(strNum[i])
    return total
def ConvertNumToStr(number):
    return(str(number))
    
#print(type(ConvertNumToStr(2)))

aws = ConvertNumToStr(2**1000)
totalSum = SumOfDigits(aws)

print(totalSum)
